<h1>New Offer</h1>

<p id="notice"><%= notice %></p>

<h1>Offers</h1>

<table>
  <thead>
    <tr>
      <th>Estado</th>
      <th>Agency</th>
      <th>Tipo</th>
      <th>App</th>
      <th>Os</th>
      <th>Geo</th>
      <th>Price</th>
      <th>Agencylink</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offersI.each do |offer| %>
      <tr>
        <td><%= offer.estado %></td>
        <td><%= offer.agency %></td>
        <td><%= offer.tipo %></td>
        <td><%= offer.app %></td>
        <td><%= offer.os %></td>
        <td><%= offer.geo %></td>
        <td><%= offer.price %></td>
        <td><%= offer.agencyLink %></td>
        <td><%= link_to 'Show', offer %></td>
        <td><%= link_to 'Edit', edit_offer_path(offer) %></td>
        <td><%= link_to 'Destroy', offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= render 'form', offer: @offer %>

<%= link_to 'Back', offers_path %>
