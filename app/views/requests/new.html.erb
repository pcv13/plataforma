<h1>New Request</h1>

<p id="notice"><%= notice %></p>

<h1>Requests</h1>

<table>
  <thead>
    <tr>
      <th>Estado</th>
      <th>Agency</th>
      <th>Tipo</th>
      <th>App</th>
      <th>Os</th>
      <th>Geo</th>
      <th>Price</th>
      <th>Agencylink</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requestsI.each do |request| %>
      <tr>
        <td><%= request.estado %></td>
        <td><%= request.agency %></td>
        <td><%= request.tipo %></td>
        <td><%= request.app %></td>
        <td><%= request.os %></td>
        <td><%= request.geo %></td>
        <td><%= request.price %></td>
        <td><%= request.agencyLink %></td>
        <td><%= link_to 'Show', request %></td>
        <td><%= link_to 'Edit', edit_request_path(request) %></td>
        <td><%= link_to 'Destroy', request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= render 'form', request: @request %>

<%= link_to 'Back', requests_path %>
