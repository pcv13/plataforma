

<div class= "bg">
<p id="notice"><%= notice %></p>
<nav class="select  ">
    <li><a href="#">update</a></li>
    <li><a href="/requests">Request</a></li>
    <li><a href="/offers">Board</a></li>
    <li><a href="/offers/list">List</a></li>
</nav>



<h1>REQUEST</h1>
<table class="container" cellpandding="0" cellspacing="0" border="0">
  <thead>
    <tr>
      <th><h1>Agency</h1></th>
      <th><h1>Tipo</h1></th>
      <th><h1>App</h1></th>
      <th><h1>Os</h1></th>
      <th><h1>Geo</h1></th>
      <th><h1>Price</h1></th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.estado %></td>
        <td><%= request.agency %></td>
        <td><%= request.tipo %></td>
        <td><%= request.app %></td>
        <td><%= request.os %></td>
        <td><%= request.geo %></td>
        <td><%= request.price %></td>
        <td><%= link_to 'Show', request %></td>
        <td><%= link_to 'Edit', edit_request_path(request) %></td>
        <td><%= link_to 'Destroy', request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div class="center">
  <%= render 'form', request: @request %>

</div>
</div>
